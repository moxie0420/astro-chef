{
  "name": "@astrochef/backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build"
  },
  "nx": {
    "targets": {
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@astrochef/backend:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@astrochef/backend:build:development"
          },
          "production": {
            "buildTarget": "@astrochef/backend:build:production"
          }
        }
      }
    }
  },
  "devDependencies": {
    "@rsbuild/core": "^1.3.7"
  }
}
